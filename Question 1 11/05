#include <iostream>
#include <string>
using namespace std;

int main() {
    int n;
    cout << "Enter number of strings: ";
    cin >> n;
    cin.ignore();  

    string** arr = new string*[n];   // array of pointers

    for(int i = 0; i < n; i++) {
        arr[i] = new string;         // allocate each string
        cout << "Enter string " << i+1 << ": ";
        getline(cin, *arr[i]);
    }

    cout << "\nStored Strings:\n";
    for(int i = 0; i < n; i++)
        cout << *arr[i] << endl;

    // Free memory
    for(int i = 0; i < n; i++)
        delete arr[i];

    delete[] arr;

    return 0;
}
